-var menu = ['Главная','Новости','Публикации','О компании','Контакты'];
header
	.menu-line
		.white-block
		.container
			.navigation-wrap
				menu.main-menu
					-for (var i = 0; i < menu.length; i++)
						li.main-menu__li
							a(href='#' alt='#{menu[i]}') #{menu[i]}
				form.search
					input.search_input(name='search_query' type="text")
					button.search_subm(type='submit')
	.container.logo-line
